CREATE TABLE Musteriler (
    musteri_id INT PRIMARY KEY,
    ad VARCHAR(50),
    soyad VARCHAR(50),
    adres VARCHAR(100),
    telefon VARCHAR(15)
);

INSERT INTO Musteriler (musteri_id, ad, soyad, adres, telefon)
